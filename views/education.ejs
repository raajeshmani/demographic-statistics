    <br> <br><br><br><br>

              <div class="container" id="create">
              <form name="Create" class="form-horizontal" action="/addeducation" method="POST">

                  <input style="display:none" name="aadhaarID" value="<%=aadhaarID%>">
                  <div class="form-inline justify-content-center">
                      <label class="control-label col-sm-2"  for="primary">Primary Education :</label>
                      <div class="col-sm-6">
                          <input type="text" name="primary" id="primary" 
                          class="form-control" placeholder="Enter primary Education" >
                      </div>
                  </div><br>

                  <div class="form-inline justify-content-center">
                      <label class="control-label col-sm-2" for="secondary">Secondary Education :</label>
                      <div class="col-sm-6">
                          <input type="text" name="secondary" id="secondary"
                          class="form-control" placeholder="Enter Secondary Education" >
                      </div>
                  </div><br>

                  <div class="form-inline justify-content-center">
                      <label class="control-label col-sm-2" for="college">College Education :</label>
                      <div class="col-sm-6">
                          <input type="text" name="college" id="college"
                          class="form-control" placeholder="Enter College Education" >
                      </div>
                  </div><br>

                  <div class="form-inline justify-content-center">
                      <label class="control-label col-sm-2" for="doctrate">Doctrate Education :</label>
                      <div class="col-sm-6">
                          <input type="text" name="doctrate" id="doctrate"
                          class="form-control" placeholder="Enter Doctrate Education" >
                      </div>
                  </div><br>

                  <div class="form-inline justify-content-center">
                      <div class="col-sm-offset-2 col-sm-6">
                          <button name="isSkip" type="submit" class="btn btn-primary" value="false">Submit</button>
                          <button name="isSkip" type="submit" class="btn btn-warning" value="true">No Education? Skip</button>

                      </div>
                  </div>
              </form>


          </div>


</body>

</html>
